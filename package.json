{
  "name": "portfolio-v3",
  "version": "2.0.0",
  "private": true,
  "description": "Showcasing his skills through jobs, projects, school transcripts, community service, and life experiences.",
  "author": "Kalen Wallin <kalenwallin1@gmail.com>",
  "repository": "kalenwallin/portfolio.v3",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "deps:upgrade": "[ -z $GITHUB_ACTIONS ] && pnpm up -L notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && run-s deps:link:* || echo 'Skipping deps:update on CI'",
    "deps:unlink": "[ -z $GITHUB_ACTIONS ] && pnpm add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link:notion-types": "pnpm link ../react-notion-x/packages/notion-types",
    "deps:link:notion-utils": "pnpm link ../react-notion-x/packages/notion-utils",
    "deps:link:notion-client": "pnpm link ../react-notion-x/packages/notion-client",
    "deps:link:react-notion-x": "pnpm link ../react-notion-x/packages/react-notion-x",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@keyvhq/core": "^1.6.9",
    "@keyvhq/redis": "^1.6.10",
    "@react-icons/all-files": "^4.1.0",
    "@upstash/redis": "^1.22.0",
    "@vercel/analytics": "^0.1.11",
    "@vercel/og": "^0.0.19",
    "classnames": "^2.5.1",
    "expiry-map": "^2.0.0",
    "ky": "^1.7.4",
    "lqip-modern": "^2.2.1",
    "next": "^15.1.5",
    "notion-client": "^7.1.6",
    "notion-types": "^7.1.6",
    "notion-utils": "^7.1.6",
    "p-map": "^7.0.3",
    "p-memoize": "^7.1.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-body-classname": "^1.3.1",
    "react-dom": "^18.2.0",
    "react-notion-x": "^7.2.6",
    "react-tweet": "^3.2.1",
    "react-use": "^17.6.0",
    "rss": "^1.2.2",
    "tsup": "^8.3.5"
  },
  "devDependencies": {
    "@fisch0920/eslint-config": "^1.4.0",
    "@next/bundle-analyzer": "^15.1.5",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
}